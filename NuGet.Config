<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
        <!-- 添加 Dalamud SDK 的源 -->
        <add key="Dalamud" value="https://goatcorp.github.io/dalamud-nuget/v2" />
    </packageSources>
    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
        <packageSource key="Dalamud">
            <package pattern="Dalamud.*" />
        </packageSource>
    </packageSourceMapping>
</configuration>
